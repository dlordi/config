[init]
	defaultBranch = main
[alias]
	staash = stash --all
	aaadd = add -A
	cam = commit -am
	l = log --oneline
	lol = log --pretty=format:'%C(yellow)%h%Creset %C(magenta)%ad%Creset %s' --date=format-local:'%d/%m/%Y %H:%M:%S'
	s = status
	pom = push origin main
	puom = pull origin main
	cob = checkout -b
[grep]
	lineNumber = true
[color]
	ui = auto
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
